        "f6460894-136d-4316-82fe-5bf6189ba09d": {
          "type": "richtext",
          "settings": {
            "content": "<p><strong>25 Jahre Garantie<\/strong><br\/>Wir geben auf alle unsere Solarmodule 25 Jahre Garantie für extra lineare Ausgangsleistung und 12 Jahre Garantie auf Material und Verarbeitung.<\/p>"
          }
        }
      },
